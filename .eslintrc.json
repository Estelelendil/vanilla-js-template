{
  "parser": "@babel/eslint-parser",
  "extends": "airbnb-base",
  "plugins": ["@babel"],
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "@babel/semi": 1
  }
}